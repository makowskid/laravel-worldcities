{
  "name": "coldcoder/laravel-worldcities",
  "description": "A Laravel package to retrieve cities in the world",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ou Junhui",
      "email": "solaze@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4||^8.0",
    "illuminate/container": "^9.0",
    "illuminate/contracts": "^9.0",
    "illuminate/database": "^9.0",
    "illuminate/support": "^9.0",
    "spatie/laravel-translatable": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2|^7.0|^8.0|^9.0",
    "orchestra/testbench": "~3.5.0|~3.6.0|^4.0|^5.0|^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Coldcoder\\WorldCity\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Coldcoder\\WorldCity\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Coldcoder\\WorldCity\\WorldCityServiceProvider"
      ]
    }
  }
}
